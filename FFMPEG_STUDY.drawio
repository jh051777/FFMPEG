        <mxCell id="xB45A97ij3tuCxRrMWK4-1" value="mp4 파일에서 비디오 or 오디오 추출하기" style="text;html=1;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontSize=18;fontStyle=1" parent="1" vertex="1">
